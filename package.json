{
  "name": "postwoman",
  "version": "1.9.7",
  "description": "A free, fast and beautiful API request builder",
  "author": "liyasthomas",
  "private": true,
  "scripts": {
    "predev": "node build.js --dev",
    "dev": "nuxt",
    "prebuild": "node build.js",
    "build": "nuxt build",
    "start": "nuxt start",
    "pregenerate": "node build.js",
    "generate": "nuxt generate",
    "pretty-quick": "pretty-quick --staged --pattern \"**/*.*(html|js|json|vue)\"",
    "test": ""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty-quick"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": false,
    "printWidth": 100
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.0",
    "@nuxtjs/gtm": "^2.3.0",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/toast": "^3.3.1",
    "ace-builds": "^1.4.12",
    "firebase": "^7.16.1",
    "graphql": "^15.3.0",
    "graphql-language-service-interface": "^2.4.0",
    "nuxt": "^2.13.3",
    "nuxt-i18n": "^6.13.1",
    "paho-mqtt": "^1.1.0",
    "socket.io-client": "^2.3.0",
    "socketio-wildcard": "^2.0.0",
    "v-tooltip": "^2.0.3",
    "vuejs-auto-complete": "^0.9.0",
    "vuex-persist": "^2.2.0",
    "yargs-parser": "^18.1.3"
  },
  "devDependencies": {
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "eslint": "^7.5.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "sass-loader": "^9.0.2",
    "start-server-and-test": "^1.11.2"
  }
