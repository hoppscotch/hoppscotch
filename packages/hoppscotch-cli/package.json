{
  "name": "hopp-cli",
  "version": "0.1.0",
  "description": "A CLI to run Hoppscotch test scripts in CI environments.",
  "main": "dist/index.js",
  "bin": {
    "hopp-cli": "bin/hopp-cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpx tsc --build src",
    "dev": "pnpx tsc --build src --watch",
    "debugger": "node debugger.js 9999",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "keywords": [
    "cli",
    "hoppscotch",
    "hopp-cli"
  ],
  "author": "Gita Alekhya Paul for Hoppscotch <54375111+gitaalekhyapaul@users.noreply.github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:gitaalekhyapaul/hopp-cli.git"
  },
  "private": false,
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^7.3.3",
    "@types/inquirer-fuzzy-path": "^2.3.1",
    "@types/table": "^6.3.2",
    "esm": "^3.2.25",
    "prettier": "^2.5.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@hoppscotch/data": "workspace:^0.3.0",
    "@hoppscotch/js-sandbox": "workspace:^1.0.0",
    "axios": "^0.21.4",
    "chalk": "^4.1.1",
    "commander": "^8.0.0",
    "fp-ts": "^2.11.3",
    "inquirer": "^8.1.1",
    "inquirer-fuzzy-path": "^2.3.0",
    "table": "^6.7.1"
  }
}
